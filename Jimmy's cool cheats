local Tab3 = Window:MakeTab({
    Name = "Trolling",
    Icon = "rbxassetid://4743180488",
    PremiumOnly = false
})

Tab3:AddLabel("WARNING: YOU NEED TO USE BLADE MASTER TO THIS TAB TO WORK")

local victimDropdown = Tab3:AddDropdown({
    Name = "Select Victim",
    Default = "None",
    Options = {},
    Callback = function(value)
        victimName = value
    end
})

local function updateVictimList()
    local players = game.Players:GetPlayers()
    local options = {}
    for _, player in ipairs(players) do
        if player ~= game.Players.LocalPlayer then
            table.insert(options, player.Name)
        end
    end
    victimDropdown:Refresh(options, true)
end

game.Players.PlayerAdded:Connect(updateVictimList)
game.Players.PlayerRemoving:Connect(updateVictimList)
updateVictimList()

local savedPosition = nil
Tab3:AddButton({
    Name = "Save Position",
    Callback = function()
        if character:FindFirstChild("HumanoidRootPart") then
            savedPosition = character.HumanoidRootPart.CFrame
        end
    end
})

local function checkAndMaintainPosition(victimRootPart)
    local startTime = tick()
    while tick() - startTime < 1 do
        wait(0.000001)
        local direction = (victimRootPart.Position - character.HumanoidRootPart.Position).unit
        character.HumanoidRootPart.CFrame = CFrame.new(victimRootPart.Position - direction * 5, victimRootPart.Position)
    end
end

Tab3:AddButton({
    Name = "Bring victim",
    Callback = function()
        secondTool.Parent = character
        if character:FindFirstChild("HumanoidRootPart") then
            local victim = game.Players:FindFirstChild(victimName)
            if victim and victim.Character and victim.Character:FindFirstChild("HumanoidRootPart") then
                local victimRootPart = victim.Character.HumanoidRootPart
                local originalCFrame = character.HumanoidRootPart.CFrame
                
                checkAndMaintainPosition(victimRootPart)
                wait(1)
                if savedPosition then
                    character.HumanoidRootPart.CFrame = savedPosition
                else
                    character.HumanoidRootPart.CFrame = originalCFrame
                end
            end
        end
    end
})

local autoFarmActive = false
Tab3:AddToggle({
    Name = "Autofarm",
    Default = false,
    Callback = function(toggled)
        autoFarmActive = toggled
        spawn(function()
            while autoFarmActive do
                local victim = game.Players:FindFirstChild(victimName)
                if victim and victim.Character and victim.Character:FindFirstChild("HumanoidRootPart") then
                    if character:FindFirstChild("HumanoidRootPart") then
                        character.HumanoidRootPart.CFrame = CFrame.new(victim.Character.HumanoidRootPart.Position - Vector3.new(0, 0, 2))
                    end
                    wait(0.00000001)
                end
                wait(0.1)
            end
        end)
    end
})

local Tab4 = Window:MakeTab({
    Name = "Fun",
    Icon = "rbxassetid://4483345998",
    PremiumOnly = false
})

local touchFlingActive = false
Tab4:AddToggle({
    Name = "Touch Fling",
    Default = false,
    Callback = function(toggled)
        touchFlingActive = toggled
        spawn(function()
            while touchFlingActive do
                local humanoidRootPart = character:FindFirstChild("HumanoidRootPart")
                if humanoidRootPart then
                    local humanoidRootPartSize = humanoidRootPart.Size
                    local touchPart = Instance.new("Part", character)
                    touchPart.Size = humanoidRootPartSize
                    touchPart.CFrame = humanoidRootPart.CFrame
                    touchPart.Transparency = 1
                    touchPart.CanCollide = false
                    touchPart.Anchored = true

                    local function onTouch(hit)
                        if hit and hit.Parent and hit.Parent:IsA("Model") and hit.Parent:FindFirstChildOfClass("Humanoid") then
                            local targetHumanoidRootPart = hit.Parent:FindFirstChild("HumanoidRootPart")
                            if targetHumanoidRootPart then
                                local spinDuration = 4
                                local spinEndTime = tick() + spinDuration
                                local originalCFrame = character.HumanoidRootPart.CFrame
                                while tick() < spinEndTime do
                                    character.HumanoidRootPart.CFrame = CFrame.new(targetHumanoidRootPart.Position) * CFrame.Angles(0, math.rad(), 0)
                                    wait()
                                end
                                character.HumanoidRootPart.CFrame = originalCFrame
                            end
                        end
                    end
                    touchPart.Touched:Connect(onTouch)

                    wait(0.1)
                end
            end
        end)
    end
})

local Tab5 = Window:MakeTab({
    Name = "Settings",
    Icon = "rbxassetid://settings_icon_asset_id",
    PremiumOnly = false
})

Tab5:AddButton({
    Name = "Low Graphics",
    Callback = function()
        game.Lighting.Brightness = 1
        game.Lighting.GlobalShadows = false
        game.Lighting.ClockTime = 12
        game.Lighting.FogEnd = 100000
        game.Lighting.FogStart = 100000
        game.Lighting.OutdoorAmbient = Color3.fromRGB(128, 128, 128)
    end
})

Tab5:AddButton({
    Name = "High Graphics",
    Callback = function()
        game.Lighting.Brightness = 2
        game.Lighting.GlobalShadows = true
        game.Lighting.ClockTime = 14
        game.Lighting.FogEnd = 100000
        game.Lighting.FogStart = 100000
        game.Lighting.OutdoorAmbient = Color3.fromRGB(255, 255, 255)
    end
})

OrionLib:Init()
