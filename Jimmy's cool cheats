local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local player = game.Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local backpack = player.Backpack
local firstTool = backpack:GetChildren()[1]
local secondTool = backpack:GetChildren()[2]
local thirdTool = backpack:GetChildren()[3]
local fourthTool = backpack:GetChildren()[4]

local Window = OrionLib:MakeWindow({
    Name = "Jimmy's Cool The Strongest Battlegrounds Cheats",
    HidePremium = false,
    SaveConfig = true,
    ConfigFolder = "JimmyScripts"
})

local Tab1 = Window:MakeTab({
    Name = "Main",
    Icon = "rbxassetid://6693712950",
    PremiumOnly = false
})

local antiStunActive = false
Tab1:AddToggle({
    Name = "Anti-Stun | TRYS to make you move while still stunned",
    Default = false,
    Callback = function(toggled)
        antiStunActive = toggled
        spawn(function()
            while antiStunActive do
                if character:FindFirstChildOfClass("Humanoid") then
                    character.Humanoid.WalkSpeed = 25
                end
                wait(0)
            end
        end)
    end
})

local antiRagdollActive = false
local lastPosition = nil
Tab1:AddToggle({
    Name = "Anti-Ragdoll",
    Default = false,
    Callback = function(toggled)
        antiRagdollActive = toggled
        spawn(function()
            while antiRagdollActive do
                if character:FindFirstChildOfClass("HumanoidRootPart") then
                    if character.HumanoidRootPart:FindFirstChild("Ragdoll") then
                        if lastPosition then
                            character.HumanoidRootPart.CFrame = lastPosition
                        end
                    else
                        lastPosition = character.HumanoidRootPart.CFrame
                    end
                end
                wait(0.1)
            end
        end)
    end
})

local antiFlingActive = false
Tab1:AddToggle({
    Name = "Anti-Fling",
    Default = false,
    Callback = function(toggled)
        antiFlingActive = toggled
        spawn(function()
            while antiFlingActive do
                if character:FindFirstChildOfClass("HumanoidRootPart") then
                    local humanoidRootPart = character.HumanoidRootPart
                    humanoidRootPart.Velocity = Vector3.new(0, 0, 0)
                end
                wait(0.1)
            end
        end)
    end
})

local predictMovementActive = false
Tab1:AddToggle({
    Name = "Predict Opponent Movement",
    Default = false,
    Callback = function(toggled)
        predictMovementActive = toggled
        if toggled then
            spawn(function()
                while predictMovementActive do
                    local closestDistance = math.huge
                    local closestPlayer = nil
                    for _, player in ipairs(game.Players:GetPlayers()) do
                        if player ~= game.Players.LocalPlayer and player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
                            local distance = (player.Character.HumanoidRootPart.Position - character.HumanoidRootPart.Position).Magnitude
                            if distance < closestDistance then
                                closestDistance = distance
                                closestPlayer = player
                            end
                        end
                    end

                    if closestPlayer and closestPlayer.Character then
                        local rootPart = closestPlayer.Character:FindFirstChild("HumanoidRootPart")
                        if rootPart then
                            local velocity = rootPart.Velocity
                            local prediction = rootPart.Position + velocity * 0.5
                            character.HumanoidRootPart.CFrame = CFrame.new(character.HumanoidRootPart.Position, prediction)
                        end
                    end
                    wait(0.1)
                end
            end)
        end
    end
})

local aimlockActive = false
Tab1:AddToggle({
    Name = "Aimlock Nearest Person",
    Default = false,
    Callback = function(toggled)
        aimlockActive = toggled
        if toggled then
            spawn(function()
                while aimlockActive do
                    local closestDistance = math.huge
                    local closestPlayer = nil
                    for _, player in ipairs(game.Players:GetPlayers()) do
                        if player ~= game.Players.LocalPlayer and player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
                            local distance = (player.Character.HumanoidRootPart.Position - character.HumanoidRootPart.Position).Magnitude
                            if distance < closestDistance then
                                closestDistance = distance
                                closestPlayer = player
                            end
                        end
                    end

                    if closestPlayer and closestPlayer.Character then
                        local rootPart = closestPlayer.Character:FindFirstChild("HumanoidRootPart")
                        if rootPart then
                            character.HumanoidRootPart.CFrame = CFrame.new(character.HumanoidRootPart.Position, rootPart.Position)
                        end
                    end
                    wait(0.1)
                end
            end)
        end
    end
})

